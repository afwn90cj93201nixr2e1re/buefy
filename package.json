{
  "name": "buefy",
  "version": "0.8.2",
  "homepage": "https://buefy.org",
  "description": "Lightweight UI components for Vue.js based on Bulma",
  "author": "Rafael Beraldo <rafael.pimpa@gmail.com>",
  "maintainers": [
    "Walter Tommasi <tommsi20@gmail.com>"
  ],
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/buefy.min.js",
  "typings": "types/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/buefy/buefy.git"
  },
  "bugs": {
    "url": "https://github.com/buefy/buefy/issues"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "lint": "eslint --ext .js,.vue src docs",
    "lint:fix": "eslint --ext .js,.vue src docs --fix",
    "lint:staged": "lint-staged",
    "unit": "jest --runInBand",
    "test": "npm run lint && npm run unit",
    "test:watch": "npm run unit -- --watchAll",
    "build:js": "rollup -c && rollup -c --environment MINIFY",
    "build:js:watch": "rollup -c --watch",
    "build:scss": "node-sass --output-style expanded src/scss/buefy-build.scss | node ./build/banner.js > dist/buefy.css && cleancss -o dist/buefy.min.css dist/buefy.css",
    "build:scss:watch": "node-sass src/scss/buefy-build.scss --w",
    "build:lib": "npm run build:js && npm run build:scss",
	
	"this is for custom build with webpack, merge it everytime":"as is",
	"build:scss_":"node-sass src/scss/buefy-build.scss dist/buefy.css --output-style expanded",
	"build:scss_min":"cleancss -o dist/buefy.min.css dist/buefy.css",
    "build:webpack": "node build/prodonly/build-lib.js && npm run build:scss_ && npm run build:scss_min",
	"_this is for custom build with webpack, merge it everytime":"as is",
	
	"build:lib:watch": "concurrently \"npm run build:js:watch\" \"npm run build:scss:watch \" ",
    "build:docs": "node build/build.js",
    "build": "npm run build:lib && npm run build:docs",
    "cov": "./node_modules/codcov/bin/codcov"
  },
  "keywords": [
    "bulma",
    "vue",
    "vuejs",
    "vue-bulma",
    "components"
  ],
  "dependencies": {
    "bulma": "0.7.5",
    "terser-webpack-plugin": "^1.3.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "autoprefixer": "9.6.0",
    "axios": "0.19.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.4.2",
    "cleave.js": "1.5.0",
    "clipboard": "2.0.4",
    "codecov": "3.5.0",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "cross-spawn": "6.0.5",
    "css-loader": "3.0.0",
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@fortawesome/fontawesome-free": "5.3.1",
    "@mdi/font": "2.7.94",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "6.25.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "22.1.0",
    "clean-css-cli": "4.3.0",
    "eslint": "4.18.2",
    "eslint-config-buefy": "0.0.3",
    "eventsource-polyfill": "0.9.6",
    "express": "4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "4.0.0",
    "font-awesome": "4.7.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "highlight.js": "9.15.8",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "http-proxy-middleware": "0.19.1",
    "lint-staged": "8.2.1",
    "node-sass": "4.12.0",
    "open": "6.3.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "ora": "3.4.0",
    "pre-commit": "1.2.2",
    "raw-loader": "3.0.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "semver": "6.1.1",
    "shelljs": "0.8.3",
    "sortablejs": "1.9.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "2.0.0",
    "vue": "2.6.10",
    "vue-analytics": "5.17.0",
    "vue-loader": "^15.7.0",
    "vue-progressbar": "0.7.5",
    "vue-router": "3.0.6",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.35.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-dev-server": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "4.2.1",
    "rollup": "1.17.0",
    "rollup-copy-plugin": "0.1.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-vue": "5.0.0",
    "scrollreveal": "3.3.6",
    "vee-validate": "2.1.0-beta.9"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
